<UserControl x:Class="Root_MarsLogView.ListCFG_UI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Root_MarsLogView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DataGrid x:Name="dataGrid" IsReadOnly="True" AutoGenerateColumns="False">
        <DataGrid.Columns>
            <DataGridTextColumn Header="Module ID" Binding="{Binding p_sModule}"/>
            <DataGridTextColumn Header="Time" Binding="{Binding p_sTime}"/>
            <DataGridTextColumn Header="Category" Binding="{Binding p_sCategory}"/>
            <DataGridTextColumn Header="Cfg ID" Binding="{Binding p_sCfgID}"/>
            <DataGridTextColumn Header="Value" Binding="{Binding p_sValue}"/>
            <DataGridTextColumn Header="Unit" Binding="{Binding p_sUnit}"/>
            <DataGridTextColumn Header="ECID" Binding="{Binding p_sECID}"/>
            <DataGridTextColumn Header="Data" Binding="{Binding p_sData}"/>
        </DataGrid.Columns>
    </DataGrid>
</UserControl>
