<UserControl x:Class="Root_VEGA_P_Vision.ScrewReview_Panel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Root_VEGA_P_Vision"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="1000" Background="{StaticResource PanelBackGround}">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <GroupBox Grid.Column="0" Header="Time">
            <local:screwUI x:Name="screwUI_Time"/>
        </GroupBox>
        <GroupBox Grid.Column="1" Header="Illumination">
            <local:screwUI x:Name="screwUI_Illum"/>
        </GroupBox>
    </Grid>
</UserControl>
