<UserControl x:Class="Root_VEGA_P.Engineer.Recipe_Nozzle_UI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Root_VEGA_P.Engineer"
             xmlns:module="clr-namespace:Root_VEGA_P.Module"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <module:NozzleSet_UI Grid.Column="0" x:Name="nozzleSetEOPDoor" Margin="3"/>
        <module:NozzleSet_UI Grid.Column="1" x:Name="nozzleSetEIPPlate" Margin="3"/>
        <module:NozzleSet_UI Grid.Column="2" x:Name="nozzleSetEIPCover" Margin="3"/>
        <module:NozzleSet_UI Grid.Column="3" x:Name="nozzleSetEOPDome" Margin="3"/>
    </Grid>
</UserControl>
