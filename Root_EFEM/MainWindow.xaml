<Window x:Class="Root_EFEM.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Root_EFEM"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Closing="Window_Closing"
        Title="Root EFEM" Height="1000" Width="1920" WindowState="Maximized">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <Grid>
            <local:EFEM_Engineer_UI x:Name="engineerUI" Grid.Row="0"/>
        </Grid>
        <StackPanel Grid.Row="1">
            <Button x:Name="ALIDList" Content="ALIDList" Click="ALIDList_Click" Grid.Row="1" Margin="10" Height="30" Width="1900" FontSize="18" FontWeight="Bold" />
        </StackPanel>
    </Grid>
</Window>
