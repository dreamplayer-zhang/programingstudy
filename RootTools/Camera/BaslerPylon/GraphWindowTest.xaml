<Window x:Class="RootTools.Camera.BaslerPylon.GraphWindowTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RootTools.Camera.BaslerPylon"
        mc:Ignorable="d"
        Title="GraphWindowTest" Height="450" Width="800">
    <Grid>
        <local:ChannelGraph x:Name="chart" Margin="0,30,0,0"/>
        <CheckBox Content="Red" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" IsChecked="{Binding visibleRed}"/>
        <CheckBox Content="Green" HorizontalAlignment="Left" Margin="101.816,10,0,0" VerticalAlignment="Top" IsChecked="{Binding visibleGreen}"/>
        <CheckBox Content="Blue" HorizontalAlignment="Left" Margin="201.704,10,0,0" VerticalAlignment="Top" IsChecked="{Binding visibleBlue}"/>
    </Grid>
</Window>
