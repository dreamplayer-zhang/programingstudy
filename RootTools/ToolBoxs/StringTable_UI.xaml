<UserControl x:Class="RootTools.ToolBoxs.StringTable_UI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RootTools.ToolBoxs"
             xmlns:tree="clr-namespace:RootTools.Trees"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Horizontal">
            <Button x:Name="Open" Content="Open" Margin="2" MinWidth="80" Click="Open_Click"/>
            <Button x:Name="Save" Content="Save" Margin="2" MinWidth="80" Click="Save_Click"/>
        </StackPanel>
        <tree:TreeUI Grid.Row="1" x:Name="treeUI"/>
    </Grid>
</UserControl>
