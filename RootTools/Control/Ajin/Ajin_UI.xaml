<UserControl x:Class="RootTools.Control.Ajin.Ajin_UI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RootTools.Control.Ajin"
             xmlns:tree="clr-namespace:RootTools.Trees"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <TabControl>
        <TabItem Header="DIO">
            <local:AjinDIO_UI x:Name="ajinDIOUI"/>
        </TabItem>
        <TabItem Header="Axis">
            <local:AjinListAxis_UI x:Name="ajinListAxisUI"/>
        </TabItem>
        <TabItem Header="Setup">
            <tree:TreeUI x:Name="treeRootUI"/>
        </TabItem>
    </TabControl>
</UserControl>
