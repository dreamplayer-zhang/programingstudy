<UserControl x:Class="RootTools.Weigh.LoadCell_UI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RootTools.Weigh"
             xmlns:comm="clr-namespace:RootTools.Comm"
             xmlns:tree="clr-namespace:RootTools.Trees"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TabControl Grid.Row="1">
            <TabItem Header="RS232">
                <comm:RS232_UI x:Name="rs232UI"/>
            </TabItem>
            <TabItem Header="CAS Setting">
                <tree:TreeUI x:Name="treeRootUI"/>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
