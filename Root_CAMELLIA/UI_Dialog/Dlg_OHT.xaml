<Window x:Class="Root_CAMELLIA.UI_Dialog.Dlg_OHT"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:oht="clr-namespace:RootTools.OHTNew;assembly=RootTools"
        xmlns:local="clr-namespace:Root_CAMELLIA.UI_Dialog"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Closing="Window_Closing"
        Title="OHT" Height="750" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0">
            <GroupBox Grid.Column="0">
                <GroupBox.Header>
                    <TextBlock Text="LoadportA"/>
                </GroupBox.Header>
                <oht:OHT_UI x:Name="OHTA"/>
            </GroupBox>
        </Grid>
        <Grid Grid.Column="1">
            <GroupBox Grid.Column="1">
                <GroupBox.Header>
                    <TextBlock Text="LoadportB"/>
                </GroupBox.Header>
                <oht:OHT_UI x:Name="OHTB"/>
            </GroupBox>
        </Grid>
    </Grid>
</Window>
