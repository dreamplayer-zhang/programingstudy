<UserControl x:Class="Root_CAMELLIA.Camellia_ToolBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:toolBox="clr-namespace:RootTools.ToolBoxs;assembly=RootTools"
             xmlns:tools="clr-namespace:RootTools;assembly=RootTools"
             xmlns:local="clr-namespace:Root_CAMELLIA"
             xmlns:tree="clr-namespace:RootTools.Trees;assembly=RootTools"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl Grid.Column="2">
            <TabItem Header="ToolBox"
                     Background="DarkSalmon">
                <toolBox:ToolBox_UI x:Name="toolBoxUI"
                                    Margin="2" />
            </TabItem>
            <TabItem Header="Login"
                     Background="DarkGreen">
                <tools:Login_UI x:Name="loginUI" />
            </TabItem>
            <TabItem Header="Setup"
                     Background="DarkBlue">
                <tree:TreeUI x:Name="treeRootUI" />
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
