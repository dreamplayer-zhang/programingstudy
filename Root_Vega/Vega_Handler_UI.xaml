<UserControl x:Class="Root_Vega.Vega_Handler_UI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Root_Vega"
             xmlns:module="clr-namespace:RootTools.Module;assembly=RootTools"
             xmlns:gaf="clr-namespace:RootTools.GAFs;assembly=RootTools"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <TabControl x:Name="tabControl">
        <TabItem Header="ModuleList">
            <module:ModuleList_UI x:Name="moduleListUI"/>
        </TabItem>
        <TabItem Header="Recipe">
            <local:Vega_Recipe_UI x:Name="recipeUI"/>
        </TabItem>
        <TabItem Header="Process">
            <local:Vega_Process_UI x:Name="processUI"/>
        </TabItem>
        <TabItem Header="GAF">
            <gaf:GAF_UI x:Name="gafUI"/>
        </TabItem>
        <!--
        <TabItem Header="GAF_Manager">
            <module:GAF_ManagerUI  x:Name="GAFManagerUI" />
        </TabItem>
        -->
    </TabControl>
</UserControl>
