        <Converter:DictionaryItemKeysConverter x:Key="DictionaryItemKeysConverters" />
        <!--Axis:DictionaryItemGetPosConverter x:Key="DictionaryItemGetPosConverters" /-->
        <Converter:PositionScalingConverter x:Key="PositionScalingConverters"/>
        <Style TargetType="{x:Type TabControl}"
               x:Key="StackPanelTabControl">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="TabControl">
                        <Grid UseLayoutRounding="True">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="70" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Border Grid.Row="1"
                                    Margin="0,0,0,0"
                                    BorderThickness="2"
                                    BorderBrush="{StaticResource BorderBrush}">
                                <ContentPresenter ContentSource="SelectedContent" />
                            </Border>
                            <Border Grid.Row="0"
                                    Margin="0,0,0,-2"
                                    BorderBrush="{StaticResource BorderBrush}"
                                    BorderThickness="2">
                                <StackPanel IsItemsHost="True"
                                            Orientation="Horizontal"
                                            Panel.ZIndex="2"></StackPanel>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="{x:Type TabItem}"
               x:Key="StackPanelTabItem">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="TabItem">
                        <Border Name="Panel"
                                Margin="5"
                                BorderBrush="{StaticResource BorderBrush}">
                            <TextBlock VerticalAlignment="Center"
                                       HorizontalAlignment="Center"
                                       FontFamily="Arial"
                                       FontWeight="Bold"
                                       FontSize="25"
                                       Margin="5,0,5,0"
                                       Name="tb">
                                <ContentPresenter VerticalAlignment="Center"
                                                  HorizontalAlignment="Center"
                                                  ContentSource="Header" /></TextBlock>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsSelected"
                                     Value="False">
                                <Setter TargetName="Panel"
                                        Property="Background"
                                        Value="{StaticResource ResourceKey=TabBackGround}" />
                                <Setter TargetName="tb"
                                        Property="Foreground"
                                        Value="{StaticResource ResourceKey=TabForeGround}" />
                            </Trigger>
                            <Trigger Property="IsMouseOver"
                                     Value="True">
                                <Setter TargetName="Panel"
                                        Property="Background"
                                        Value="{StaticResource ResourceKey=TabMouseOver}" />
                                <Setter TargetName="tb"
                                        Property="Foreground"
                                        Value="{StaticResource ResourceKey=TabForeGround}" />
                            </Trigger>
                            <Trigger Property="IsSelected"
                                     Value="True">
                                <Setter TargetName="Panel"
                                        Property="Background"
                                        Value="{StaticResource ResourceKey=TabSelected}" />
                                <Setter TargetName="tb"
                                        Property="Foreground"
                                        Value="{StaticResource ResourceKey=TabForeGround}" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </UserControl.Resources>
