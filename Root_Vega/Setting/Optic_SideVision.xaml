                                        <Button Grid.Column="1"
                                                Style="{StaticResource BasicBtn}"
                                                Command="{Binding MRelativeMoveCommand}"
                                                Margin="5" Grid.ColumnSpan="2">
                                            <Button.Content>
                                                <StackPanel Orientation="Vertical">
                                                    <material:PackIcon Kind="ArrowLeftBox"
                                                                       Foreground="White"
                                                                       HorizontalAlignment="Center"
                                                                       Width="50"
                                                                       Height="50"></material:PackIcon>
                                                </StackPanel>
                                            </Button.Content>
                                        </Button>
                                        <Border Grid.Column="3">
                                            <TextBox VerticalAlignment="Center"
                                                     Background="{PriorityBinding}"
                                                     Text="{Binding p_dRelPos}"
                                                     Foreground="White"
                                                     HorizontalAlignment="Center"
                                                     FontSize="20"
                                                     MinWidth="40"/>
                                        </Border>
                                        <Button Grid.Column="4"
                                                Style="{StaticResource BasicBtn}"
                                                Command="{Binding PRelativeMoveCommand}"
                                                Margin="5">
                                            <Button.Content>
                                                <StackPanel Orientation="Vertical">
                                                    <material:PackIcon Kind="ArrowRightBox"
                                                                       Foreground="White"
                                                                       HorizontalAlignment="Center"
                                                                       Width="50"
                                                                       Height="50"></material:PackIcon>
                                                </StackPanel>
                                            </Button.Content>
                                        </Button>
                                    </Grid>
                                </Grid>
                            </TabItem>
                        </TabControl>
                    </Grid>
                </TabItem>
                <TabItem Header="Camera Setting"
                         Grid.Column="3"
                         DataContext="{Binding ElementName=CameraTab, Path=SelectedItem.DataContext}"
                         Style="{StaticResource OpticPylonTabItem}">
                    <Tree:TreeUI  Grid.Row="1"
                                  Background="White"
                                  DataContext="{Binding p_treeRoot}"></Tree:TreeUI>
                </TabItem>
            </TabControl>
        </Grid>
    </Grid>
</UserControl>
